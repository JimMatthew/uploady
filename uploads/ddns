
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'myddns_ipv4'
	option interface 'wan'
	option ip_source 'network'
	option ip_network 'wan'
	option enabled '1'
	option lookup_host 'jlindstrom.dynu.net'
	option update_url 'http://api.dynu.com/nic/update?hostname=[DOMAIN]&myip=[IP]&username=[USERNAME]&password=[PASSWORD]'
	option domain 'jlindstrom.dynu.netxample.com'
	option username 'jlindstrom'
	option password 'Uqj3w999'

